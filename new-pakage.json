{
    "name": "combined-project",
    "version": "1.0.0",
    "description": "Combined project of VoiceBot Reference Implementation for AudioConnector Servers and OpenAI Realtime Console.",
    "private": true,
    "author": "Kevin Wiegand <kevin.wiegand@genesys.com>",
    "main": "./dist/index.js",
    "bin": {
      "audioconnector-server-reference-implementation": "dist/index.js"
    },
    "files": [
      "dist/**/*",
      "!dist/**/*.index.js"
    ],
    "engines": {
      "node": ">=18"
    },
    "scripts": {
      "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
      "start:server": "ts-node ./src/index.ts",
      "start:client": "react-scripts start",
      "clean": "rimraf ./dist/",
      "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/__version.ts",
      "build": "npm run clean && tsc && react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "zip": "zip -r realtime-api-console.zip . -x 'node_modules' 'node_modules/*' 'node_modules/**' '.git' '.git/*' '.git/**' '.DS_Store' '*/.DS_Store' 'package-lock.json' '*.zip' '*.tar.gz' '*.tar' '.env'",
      "relay": "nodemon ./relay-server/index.js"
    },
    "dependencies": {
      "@openai/realtime-api-beta": "github:openai/openai-realtime-api-beta",
      "dotenv": "^16.4.5",
      "express": "^4.18.2",
      "iso8601-duration": "^1.3.0",
      "leaflet": "^1.9.4",
      "lodash": "^4.17.21",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-feather": "^2.0.10",
      "react-leaflet": "^4.2.1",
      "react-scripts": "^5.0.1",
      "sass": "^1.78.0",
      "save": "^2.9.0",
      "uuid": "^9.0.0",
      "web-vitals": "^2.1.4",
      "websocket": "^1.0.34",
      "ws": "^8.18.0"
    },
    "devDependencies": {
      "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
      "@testing-library/jest-dom": "^5.17.0",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "@types/express": "^4.17.17",
      "@types/jest": "^27.5.2",
      "@types/leaflet": "^1.9.12",
      "@types/lodash": "^4.14.178",
      "@types/node": "^18.18.0",
      "@types/react": "^18.3.5",
      "@types/react-dom": "^18.3.0",
      "@types/uuid": "^9.0.1",
      "@types/websocket": "^1.0.5",
      "@types/ws": "^8.5.4",
      "concurrently": "^8.2.0",
      "nodemon": "^3.1.7",
      "rimraf": "^3.0.2",
      "ts-node": "^10.4.0",
      "typescript": "^4.9.5"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "license": "ISC"
  }
  